{
  "name": "isomorphic-redux-app",
  "version": "0.1.1",
  "description": "An example of a isomorphic Redux application",
  "scripts": {
    "slate": "rm -rf node_modules && npm install",
    "clean": "rm -rf dist",
    "start": "NODE_ENV=development node src/server/index.js --progress --colors --profile",
    "start-prod": "NODE_ENV=production node src/server/index.js --progress --colors --profile",
    "build": "npm run clean && NODE_ENV=production webpack -p --progress --colors --profile",
    "test": "NODE_ENV=test mocha --compilers js:babel-core/register --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caljrimmer/isomorphic-redux-app.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "axios": "^1.3.0",
    "classnames": "2.1.5",
    "express": "^4.18.2",
    "history": "^1.17.0",
    "react": "^18.2.0",
    "react-redux": "2.1.2",
    "react-router": "1.0.0-rc1",
    "redux": "3.0.0",
    "redux-router": "1.0.0-beta3",
    "redux-thunk": "0.1.0",
    "redux-undo": "0.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^9.1.2",
    "babel-plugin-react-transform": "1.1.0",
    "babel-runtime": "5.8.20",
    "css-loader": "^6.7.3",
    "expect": "1.12.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^6.2.0",
    "jsdom-no-contextify": "3.1.0",
    "merge": "^2.1.1",
    "mocha": "^10.2.0",
    "react-transform-catch-errors": "1.0.0",
    "react-transform-hmr": "1.0.0",
    "redbox-react": "1.1.0",
    "redux-devtools": "2.1.5",
    "redux-logger": "2.0.2",
    "style-loader": "0.8.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-dev-middleware": "1.2.0",
    "webpack-hot-middleware": "2.2.0"
  }
}
